{
  "name": "quotefast-dashboard",
  "description": "AI-Powered Quote Generation & Business Management",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next out dist",
    "preview": "npm run build && npm run start",
    "check": "npm run type-check && npm run lint && npm run test:run",
    "test:unit": "vitest run --reporter=verbose",
    "test:integration": "vitest run --reporter=verbose tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:ci": "npm run test:run -- --reporter=json --outputFile=test-results.json",
    "test:lighthouse": "node scripts/lighthouse-test.js",
    "test:accessibility": "playwright test --grep @accessibility",
    "test:performance": "playwright test --grep @performance",
    "test:visual": "playwright test --grep @visual",
    "setup:supabase": "node scripts/setup-supabase.js",
    "dev:reset": "npm run clean && npm run setup:supabase && npm run dev"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-email/components": "^0.0.26",
    "@stripe/stripe-js": "^8.1.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/node": "^20.19.21",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/stripe": "^8.0.416",
    "@vercel/sdk": "^1.13.9",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.18.2",
    "inngest": "^3.44.3",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.441.0",
    "next": "14.2.5",
    "next-auth": "^4.24.11",
    "numeral": "^2.0.6",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.65.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.4",
    "stripe": "^19.1.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.5",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.57.1",
    "eslint-config-next": "16.0.0",
    "jsdom": "^27.0.0",
    "playwright": "^1.56.1",
    "vitest": "^3.2.4"
  }
}
